{
  "name": "carparkjs",
  "version": "1.0.0",
  "description": "Car Park Example",
  "main": "main.js",
  "scripts": {
    "start-cli": "node main.js",
    "start-server": "node carpark",
    "test": "jest --coverage",
    "watch": "jest --watchAll --coverage",
    "uu": "start-server-and-test start-server http://localhost:3000 uu:test",
    "uu:test": "nav-ally -r yes -f ./src/test/carpark/accessibility/carpark.yml",
    "pact:test": "jest --runInBand --setupFiles ./src/test/carpark/pact/pactSetup.js --testRegex \"/*(.test.pact.js)\"",
    "pact:publish": "node ./src/test/carpark/pact/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dervism/carparkjs"
  },
  "author": {
    "name": "Dervis Mansuroglu",
    "url": "https://github.com/dervism"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "config": {
    "pact_do_not_track": true
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "http-server": "0.11.1",
    "nav-ally": "1.3.1"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^8.2.4",
    "axios": "^0.19.0",
    "jest": "24.8.0",
    "start-server-and-test": "1.9.1"
  }
}
